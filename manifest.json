{
    "name": "Color Picker",
    "description": "A Color Picker Extension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions":[
        "activeTab",
        "scripting"
    ],
    "options_page": "option.html",
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/img/logo16.png",
        "32": "/img/logo32.png",
        "48": "/img/logo48.png"
    }
}